#version 330 core

//------------ LAYOUT  START ------------
layout (location = 0) in vec3 pos;
//------------ LAYOUT    END ------------


//------------ UNIFORM START ------------
//------------ UNIFORM   END ------------


//------------ INOUT   START ------------
out vec4 FragColor;
